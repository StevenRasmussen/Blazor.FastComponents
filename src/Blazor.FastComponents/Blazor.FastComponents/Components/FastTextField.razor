@inherits FastComponentBase

<fast-text-field @attributes="@AdditionalAttributes"
    autofocus=@AutoFocus
    readonly=@ReadOnly
    placeholder=@PlaceHolder
    required=@Required
    value=@Value
    disabled=@Disabled
    list=@List
    maxLength=@MaxLength
    minLength=@MinLength
    name=@Name
    pattern=@Pattern
    size=@Size
    spellCheck=@SpellCheck
    type=@TextFieldType.ToString()
    appearance=@Appearance.ToString()
>
    
</fast-text-field>